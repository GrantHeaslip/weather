<!DOCTYPE html>
<html lang="en-CA">
<head>
    <meta charset="utf-8">

    <!-- weather <%- appVersion %> -->

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-title" content="Weather">

    <title>Weather</title>
    <meta name="og:title" content="Weather" />
    <meta name="twitter:title" content="Weather" />
    <% let metaDescription = 'Concise interface for Environment Canada weather forecasts' %>
    <meta name="description" content="<%= metaDescription %>" />
    <meta name="og:description" content="<%= metaDescription %>" />
    <meta name="twitter:description" content="<%= metaDescription %>" />
    <% if (typeof canonicalUrl !== 'undefined') { %>
        <link rel="canonical" href="<%= canonicalUrl %>" />
        <meta name="og:url" content="<%= canonicalUrl %>" />
    <% } %>
    <meta property="og:type" content="website" />
    <meta name="author" content="Grant Heaslip" />
    <meta name="twitter:card" content="summary" />

    <link rel="stylesheet" type="text/css" media="screen" href="<%- staticPath('weather.css') %>">

    <!-- realfavicongenerator.net favicons -->
    <link rel="apple-touch-icon" sizes="57x57" href="<%- staticPath('favicons/apple-touch-icon-57x57.png') %>">
    <link rel="apple-touch-icon" sizes="60x60" href="<%- staticPath('favicons/apple-touch-icon-60x60.png') %>">
    <link rel="apple-touch-icon" sizes="72x72" href="<%- staticPath('favicons/apple-touch-icon-72x72.png') %>">
    <link rel="apple-touch-icon" sizes="76x76" href="<%- staticPath('favicons/apple-touch-icon-76x76.png') %>">
    <link rel="apple-touch-icon" sizes="114x114" href="<%- staticPath('favicons/apple-touch-icon-114x114.png') %>">
    <link rel="apple-touch-icon" sizes="120x120" href="<%- staticPath('favicons/apple-touch-icon-120x120.png') %>">
    <link rel="apple-touch-icon" sizes="144x144" href="<%- staticPath('favicons/apple-touch-icon-144x144.png') %>">
    <link rel="apple-touch-icon" sizes="152x152" href="<%- staticPath('favicons/apple-touch-icon-152x152.png') %>">
    <link rel="apple-touch-icon" sizes="180x180" href="<%- staticPath('favicons/apple-touch-icon-180x180.png') %>">
    <link rel="icon" type="image/png" href="<%- staticPath('favicons/favicon-32x32.png') %>" sizes="32x32">
    <link rel="icon" type="image/png" href="<%- staticPath('favicons/favicon-194x194.png') %>" sizes="194x194">
    <link rel="icon" type="image/png" href="<%- staticPath('favicons/favicon-96x96.png') %>" sizes="96x96">
    <link rel="icon" type="image/png" href="<%- staticPath('favicons/android-chrome-192x192.png') %>" sizes="192x192">
    <link rel="icon" type="image/png" href="<%- staticPath('favicons/favicon-16x16.png') %>" sizes="16x16">
    <link rel="manifest" href="<%- staticPath('favicons/manifest.json') %>">
    <link rel="shortcut icon" href="<%- staticPath('favicons/favicon.ico') %>">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-TileImage" content="<%- staticPath('favicons/mstile-144x144.png') %>">
    <meta name="msapplication-config" content="<%- staticPath('favicons/browserconfig.xml') %>">
    <meta name="theme-color" content="#ffffff">
    <!-- /realfavicongenerator.net favicons -->
</head>
